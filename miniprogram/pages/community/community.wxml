		</view>
		<scroll-view scroll-y="true" style="height:785rpx;">
			<view wx:for="{{unreadMess}}">
				<view class="message" bindtap="onTapProjectMessage" data-proid="{{item.proid}}">
					<image class="commentUserAvatar" src="{{item.userInfo.avatarUrl}}" bindTap="enterUserDetail"
						data-userid="item.user_id"></image>
					<view class="commentUserName">{{item.userInfo.nickName}}</view>
					<view class="commentUserName" style="color:#777"> {{item.time}}</view>
					<view class="commentContent">{{item.content}}</view>
					<image wx:if="{{item.needImg}}" z-index="500" class="image-container" style="display:block;margin-left:20rpx;"
						src="{{item.imgID}}" bindtap="onTapImg" data-imgid="{{item.imgID}}"></image>
					<view wx:if="{{item.needAgreeButton}}" bindtap="onTapMessButton" class="agreeButton"
						style="margin-left:220rpx" data-n="1" data-user="item.user_id" data-userinfo="{{item.userInfo}}"
						data-index="{{index}}">同意</view>
					<view wx:if="{{item.needAgreeButton}}" bindtap="onTapMessButton" class="agreeButton" data-n="2"
						data-user="item.user_id" data-index="{{index}}">拒绝</view>
					<view wx:if="{{item.needAgreeButton && !item.needImg}}" bindtap="onTapMessButton" class="agreeButton"
						data-user="item.user_id" data-index="{{index}}">忽略</view>
					<view wx:if="{{!item.needAgreeButton}}" bindtap="onTapMessButtonOk" class="agreeButton" data-index="{{index}}"
						style="margin-left:480rpx">好的</view>
				</view>
			</view>
			<view style="height:3rpx;width:100%;background-color:white;"></view> <!-- 分割线 -->
			<view wx:for="{{readedMess}}" style="margin-top:15rpx">
				<view class="rmessage" bindtap="onTapProjectMessage" data-proid="{{item.proid}}">
					<image class="commentUserAvatar" src="{{item.userInfo.avatarUrl}}" bindtap="enterUserDetail"
						data-userid="item.user_id"></image>
					<view class="commentUserName">{{item.userInfo.nickName}}</view>
					<view class="commentUserName" style="color:#ddd"> {{item.time}}</view>
					<view class="commentContent">{{item.content}}</view>
					<view wx:if="{{item.needAgreeButton}}" bindtap="onTaprMessButton" class="agreeButton"
						style="margin-left:350rpx" data-n="1" data-user="item.user_id" data-userinfo="{{item.userInfo}}"
						data-index="{{index}}">同意</view>
					<view wx:if="{{item.needAgreeButton}}" bindtap="onTapMessButton" class="agreeButton" data-n="2"
						data-user="item.user_id" data-index="{{index}}">拒绝</view>
				</view>
			</view>
		</scroll-view>
	</view>

	<!-- 关注用户 -->
	<view class="message-container" style="display:{{disMyPageFocus}}">
		<view class="quxiaoLine">
			<image class="quxiaoIcon2" src="../../images/quxiao.png" bindtap="onTapCancelFocusMyPage"></image>
		</view>
		<scroll-view scroll-y="true" style="height:785rpx;">
			<view style="text-align:center;margin:5rpx 0;margin-bottom:20rpx;width:100%;">
				<text style="font-size:36rpx;color:#555;margin:0 auto;">关注用户</text>
			</view>
			<view wx:for="{{favorPeoInfo}}">
				<view class="focusPeoInfoContainer">
					<view style="width:30%;display:inline-block;height:130rpx;line-height:150rpx;text-align:center;">
						<image src="{{item.avatarUrl}}" style="width:120rpx;height:120rpx;border-radius:100%;margin-top:15rpx;">
						</image>
					</view>
					<view style="width:70%;display:inline-block;height:150rpx;line-height:38rpx;padding:18rpx 0;">
						<text style="display:block;">{{item.nickName}}</text>
						<view class="myPageQualifyState">{{item.qualifyText}}
							<image wx:if="{{item.isQualified}}" src="../../images/qualify.png" class="qualifyIcon"></image>
						</view>
						<text class="focusDate" style="display:block;">关注时间：{{item.focusDate}}</text>
					</view>
				</view>
			</view>
		</scroll-view>
	</view>

	<!-- 主推项目 -->
	<view class="message-container" style="display:{{disMyPageBannerSet}}">
		<view class="quxiaoLine">
			<image class="quxiaoIcon2" src="../../images/quxiao.png" bindtap="onTapCancelBannerSetMyPage"></image>
		</view>
		<scroll-view scroll-y="true" style="height:785rpx;">
			<view wx:for="{{bannerProjects}}">
				<view class="projectContainer">
					<image class="projectAvatar" style="display:inline-block;margin:20rpx 20rpx;"
						src="{{item.pro.projectCoverImageID}}"></image>
					<view style="margin-top:18rpx">
						<text class="projectName2">{{item.pro.projectName}}</text>
						<button class="tinyButton" bindtap="onDeleteBannerProject" data-index="{{index}}">删除</button>
					</view>
				</view>
			</view>
		</scroll-view>
	</view>

	<!-- 资讯管理 -->
	<view class="message-container" style="display:{{disMyPageShares}}">
		<view class="quxiaoLine">
			<image class="quxiaoIcon2" src="../../images/quxiao.png" bindtap="onTapCancelSharesSet"></image>
		</view>
		<!-- 资讯流 -->
		<scroll-view scroll-y="true" style="width:98%;height:790rpx;">
			<view wx:for="{{myShares}}" class="share-container">
				<!-- 用户信息 头像 昵称 验证信息 -->
				<view class="userInfoContainer">
					<image class="userAvatar" src="{{item._user.avatarUrl}}" bindtap="enterUserDetail"
						data-userid="{{item._userID}}"></image>
					<text class="userName" style="top:-65rpx">{{item._user.nickName}}</text>
				</view>
				<!-- 资讯的内容 -->
				<view class="shareContent">
					<text class="shareTopic" bindtap="onClickTopic" data-topic="{{item.shareTopic}}">#{{item.shareTopic}}#</text>
					<text class="shareSubject">{{item.shareSubject}}</text>
					<text class="shareInformation">{{item.shareInformation}}</text>
					<view wx:if="{{item.imageID.length>0}}">
						<image class="image-container" style="width:160rpx;height:160rpx;" src="{{item.imageID[0]}}"></image>
						<image class="image-container" style="width:160rpx;height:160rpx;" src="{{item.imageID[1]}}"></image>
						<image class="image-container" style="width:160rpx;height:160rpx;" src="{{item.imageID[2]}}"></image>
					</view>
				</view>
				<!-- 底部按钮区 -->
				<view class="shareBottomButtonsLine" style="width:90%;">
					<view class="projectBottomButton" style="width:33%;">
						<image class="smallIcon" src="../../images/项目详情底部栏-点赞按钮（未点）@2x.png"></image>
						<text class="projectBottomNum">{{item.goodNum}}</text>
					</view>
					<!-- 发表评论按钮 -->
					<view class="projectBottomButton" style="width:33%;">
						<image class="smallIcon" src="../../images/项目详情底部栏-评论按钮@2x.png" bindtap="tapComment"
							data-shareid="{{item._id}}"></image>
						<text class="projectBottomNum">{{item.comments.length}}</text>
					</view>
					<view class="projectBottomButton" style="width:33%;">
						<image class="smallIcon" src="../../images/shanchu.png"
							style="height:45rpx;width:45rpx;position:relative;top:-3rpx;right:-10rpx" bindtap="tapDeleteShare"
							data-shareid="{{item._id}}" data-index="index"></image>
						<text class="projectBottomNum"></text>
					</view>
				</view>
			</view>
		</scroll-view>
	</view>
</view>

<!-- 项目列表 -->
<view style="display:{{disProjectList}}">
	<!--miniprogram/pages/projectList/projectList.wxml-->
	<view class="topBar">
		<image class="returnIcon" src="../../images/fanhui.png" bindtap="onUnloadProjectList"></image>
		<text class="topBarText">谷仓项目</text>
	</view>

	<!-- head -->
	<view class="head" style="background-color:#efefef;">
		<view class="divideText" style="left:-78rpx;font-weight:{{pageWeightP[0]}};border-bottom:{{pageBorderP[0]}};"
			bindtap="onTapPPageText" data-n="0">乡村教育</view>
		<view class="divideText" style="left:-26rpx;font-weight:{{pageWeightP[1]}};border-bottom:{{pageBorderP[1]}};"
			bindtap="onTapPPageText" data-n="1">乡村振兴</view>
		<view class="divideText" style="left:26rpx;font-weight:{{pageWeightP[2]}};border-bottom:{{pageBorderP[2]}};"
			bindtap="onTapPPageText" data-n="2">志愿招募</view>
		<view class="divideText" style="left:78rpx;font-weight:{{pageWeightP[3]}};border-bottom:{{pageBorderP[3]}};"
			bindtap="onTapPPageText" data-n="3">基层调研</view>
	</view>

	<!-- body -->
	<scroll-view class="scroll-body" scroll-y='true'>

		<view class="projectLabelIntro">
			<image class="labelImg" src="{{projectLabelIntroImage}}"></image>
			<view style="display:inline-block;height:140rpx;width:500rpx;margin-left:30rpx;position:relative;top:-40rpx;">
				<view style="font-size:36rpx;font-weight:700;line-height:70rpx;">{{projectLabelIntroName}}</view>
				<view style="font-size:28rpx;color:#ada59d;line-height:60rpx;">[公益时]{{projectLabelIntroVHour}}h/人</view>
			</view>
			<view class="projectLabelIntroText">
				<view>{{projectLabelIntroText}}</view>
			</view>
		</view>

		<view class="projectStream" style="margin-top:10rpx;">
			<!-- 选择显示的排序顺序 -->
			<view class="sortSelection" bindtap="onTapChooseSortRule">
				<label class="sortText" style="font-weight:{{WeightS[0]}};border-bottom:{{BorderS[0]}};"
					bindtap="onTapChooseSortRule" data-n="1">开始时间最近</label>
				<label class="sortText" style="font-weight:{{WeightS[1]}};border-bottom:{{BorderS[1]}};"
					bindtap="onTapChooseSortRule" data-n="2">热度最高</label>
				<label class="sortText" style="font-weight:{{WeightS[2]}};border-bottom:{{BorderS[2]}};"
					bindtap="onTapChooseSortRule" data-n="3">更新时间最近</label>
			</view>

			<view wx:for="{{projects}}" class="project-container">
				<!-- 点击主体部分进入详情页 -->
				<view bindtap="onTapProjectListOne" data-proid="{{item._id}}">
					<view class="projectName">
						<text>{{item.projectName}}</text>
					</view>
					<view class="projectStateLine">
						<span class="projectStateText">状态:{{item.projectState}}</span>
						<span class="projectStateText">起止时间:{{item.startDate}}~{{item.endDate}}</span>
					</view>

					<view class="projectViewCon">
						<view style="width:30%;height:210rpx;display:inline-block;">
							<image class="image-container" style="width:85%;height:82%;margin-left:20rpx"
								src='{{item.projectCoverImageID}}'></image>
						</view>
						<view style="width:70%;height:210rpx;display:inline-block;border-radius:10rpx;">
							<text class="projectInformation"
								style="width:88%;height:80%;margin-top:10rpx;">{{item.projectInformation}}</text>
						</view>
					</view>

				</view>
				<view class="projectBottomButtonsLine">
					<view class="projectBottomButton">
						<image wx:if="{{!item.isGood}}" class="smallIcon" src="../../images/项目详情底部栏-点赞按钮（未点）@2x.png"
							bindtap="tapGood" data-proid="{{item._id}}"></image>
						<image wx:if="{{item.isGood}}" class="smallIcon" src="../../images/项目详情底部栏-点赞按钮（选中）@2x.png"></image>
						<text class="projectBottomNum">{{item.goodNum}}</text>
					</view>
					<!-- 发表评论按钮 -->
					<view class="projectBottomButton">
						<image class="smallIcon" src="../../images/项目详情底部栏-评论按钮@2x.png" bindtap="tapComment"
							data-proid="{{item._id}}"></image>
						<text class="projectBottomNum">{{item.comments.length}}</text>
					</view>
					<view class="projectBottomButton">
						<image wx:if="{{!item.isCollected}}" class="smallIcon" src="../../images/项目详情底部栏-收藏按钮（未点）@2x.png"
							bindtap="tapCollect" data-proid="{{item._id}}"></image>
						<image wx:if="{{item.isCollected}}" class="smallIcon" src="../../images/项目详情底部栏-收藏按钮（选中）@2x.png"></image>
						<text class="projectBottomNum">{{item.collectNum}}</text>
					</view>
				</view>
			</view>

		</view>

	</scroll-view>
</view>

<!-- 项目详情页 -->
<view class="projectDetailPage" style="display:{{disProjectDetailPage}}">
	<view class="topBar">
		<image class="returnIcon" src="../../images/fanhui.png" bindtap="onUnloadProjectDetailToProjectList"></image>
		<text class="topBarText">项目详情</text>
		<image wx:if="{{_super}}" src="../../images/shezhi.png" class="messageIcon"
			style="left:34%;top:66rpx;width:42rpx;height:42rpx;" bindtap="onTapSetProjectToBanner"
			data-proid="{{chooseProject._id}}"></image>
	</view>

	<scroll-view class="scroll-body" scroll-y="true" style="margin-top:135rpx;height:1100rpx;">
		<view class="projectDetailHeadImgCon" bindtap="onTapChangeProjectDetailText" data-n="3">
			<!-- 两个地方都要。。。。。。。。 -->
			<swiper class="projectDetailHeadImg" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}"
				interval="{{interval}}" duration="{{duration}}" circular="true">
				<swiper-item>
					<image class="projectDetailHeadImg" src="{{chooseProject.projectCoverImageID}}"></image>
				</swiper-item>
				<view wx:for="{{chooseProject.projectImagesID}}">
					<swiper-item>
						<image class="projectDetailHeadImg" src="{{item}}" />
					</swiper-item>
				</view>
			</swiper>
		</view>
		<!--  -->
		<view style="margin-left:20rpx;">
			<view class="projectDetailLabelText">
				{{chooseProject.projectLabel}}类 公益时{{chooseProject.vHour}}
			</view>
			<view class="projectDetailName">
				{{chooseProject.projectName}}
			</view>
		</view>
		<!-- 项目简介 -->
		<view class="projectDetailInformation">
			{{chooseProject.projectInformation}}
			<image wx:if="{{isMine}}" class="myPageIcon" src="../../images/editUserWords.png"
				style="display:block;position:relative;right:-93%;margin:5rpx 0;" bindtap="onTapChangeProjectDetailText"
				data-n="1"></image>
		</view>
		<!-- 项目状态栏 -->
		<view class="projectDetailState-container">
			<view class="projectDetailState" bindtap="manageProjectMember">
				<label style="display:block;">状态</label>
				{{chooseProject.projectState}} {{projectDetailStateText}}
			</view>
			<view class="projectDetailTime">
				<label style="display:block">起止时间</label>
				<text style="font-size:25rpx;">{{chooseProject.startDate}} ~ {{chooseProject.endDate}}</text>
			</view>
		</view>
		<!-- 项目正文 -->
		<view class="projectDetailBody" style="margin-bottom:40rpx;padding-top:1rpx;">
			<view class="userInfoContainer" style="margin: 20rpx 0;">
				<image class="userAvatar" style="margin-left:0;" src="{{chooseProject.sponsorInfo.avatarUrl}}"></image>
				<text class="userName" style="top:-65rpx">{{chooseProject.sponsorInfo.nickName}}</text>
			</view>
			<view class="projectDetailContent">
				{{chooseProject.projectContent}}
				<image wx:if="{{isMine}}" class="myPageIcon" src="../../images/editUserWords.png"
					style="display:block;position:relative;right:-93%;margin:10rpx 0;" bindtap="onTapChangeProjectDetailText"
					data-n="2"></image>
			</view>
		</view>

		<view class="endLineText">已经到底了</view>
	</scroll-view>

	<view class="projectDetailInput" style="display:{{disChangeInformationOrContent}}">
		<view class="quxiaoLine">
			<image class="quxiaoIcon2" src="../../images/quxiao.png" bindtap="onTapCancelInformationOrContentInput"></image>
		</view>
		<textarea class="InformationOrContentTextarea" wx:if="{{changeInformation}}" value="{{detailTextInputSet}}"
			bindinput="onInputInformationOrContent" data-n="1"></textarea>
		<textarea class="InformationOrContentTextarea" wx:if="{{!changeInformation}}" value='{{detailTextInputSet}}'
			bindinput="onInputInformationOrContent" data-n="2"></textarea>
		<button class="smallButton" style="margin-top:10rpx;" bindtap="onTapSubmitDetailChange" data-n="1">确认</button>
	</view>

	<view class="projectDetailInputImage" style="display:{{disChangeImage}}">
		<view class="quxiaoLine">
			<image class="quxiaoIcon2" src="../../images/quxiao.png" bindtap="onTapCancelChangeImage"></image>
		</view>
		<view class="imagesInput">
			<image class="image-container" src="{{coverImage}}" bindlongtap="onLongTapCoverImg" bindtap="onTapCoverImg"
				style="display:block;margin:5rpx auto;"></image>
			<image class="image-container" src="{{projectImages[0]}}" data-n="0" bindlongtap="onLongTapImgC"
				bindtap="onTapImgC" style="display:{{disImg[0]}}"></image>
			<image class="image-container" src="{{projectImages[1]}}" data-n="1" bindlongtap="onLongTapImgC"
				bindtap="onTapImgC" style="display:{{disImg[0]}}"></image>
			<image class="image-container" src="{{projectImages[2]}}" data-n="2" bindlongtap="onLongTapImgC"
				bindtap="onTapImgC" style="display:{{disImg[0]}}"></image>
			<image class="image-container" src="{{projectImages[3]}}" data-n="3" bindlongtap="onLongTapImgC"
				bindtap="onTapImgC" style="display:{{disImg[1]}}"></image>
			<image class="image-container" src="{{projectImages[4]}}" data-n="4" bindlongtap="onLongTapImgC"
				bindtap="onTapImgC" style="display:{{disImg[1]}}"></image>
			<image class="image-container" src="{{projectImages[5]}}" data-n="5" bindlongtap="onLongTapImgC"
				bindtap="onTapImgC" style="display:{{disImg[1]}}"></image>
			<image class="image-container" src="{{projectImages[6]}}" data-n="6" bindlongtap="onLongTapImgC"
				bindtap="onTapImgC" style="display:{{disImg[2]}}"></image>
			<image class="image-container" src="{{projectImages[7]}}" data-n="7" bindlongtap="onLongTapImgC"
				bindtap="onTapImgC" style="display:{{disImg[2]}}"></image>
			<image class="image-container" src="{{projectImages[8]}}" data-n="8" bindlongtap="onLongTapImgC"
				bindtap="onTapImgC" style="display:{{disImg[2]}}"></image>
		</view>
	</view>

	<!-- 成员管理面板 -->
	<view class="message-container" style="display:{{disManageMember}}">
		<view class="quxiaoLine">
			<image class="quxiaoIcon2" src="../../images/quxiao.png" bindtap="onTapQuitManageMember"></image>
		</view>
		<scroll-view scroll-y="true" style="height:785rpx;">
			<!-- 项目发起人 -->
			<view class="member-container">
				<view class="memberTitle">
					<text style="color:#555;width:100%;text-align:center;font-size:33rpx;">队长</text>
				</view>
				<view class="memberInfo">
					<image class="memberAvatar" src="{{chooseProject.sponsorInfo.avatarUrl}}"></image>
					<view class="memberText">
						<text class="memberName">{{chooseProject.sponsorInfo.nickName}}</text>
						<text class="memberState">{{chooseProject.sponsorInfo.qualifyState}}</text>
						<view class="memberQualifyState">
							<view wx:if="{{chooseProject.sponsorInfo.isQualified}}" class="qualifyText-container">
								<text class="qualifyText">{{chooseProject.sponsorInfo.qualifyText}}</text>
								<image src="../../images/qualify.png" class="qualifyIcon"></image>
							</view>
						</view>
					</view>
				</view>
			</view>
			<!-- 队员 -->
			<view wx:for="{{chooseProject.member}}" wx:key="id" class="member-container" bindlongtap="onLongTapMember"
				data-index="{{index}}">
				<view class="memberTitle">
					<text style="color:#555;width:100%;text-align:center;font-size:33rpx;">队员</text>
				</view>
				<view class="memberInfo">
					<image class="memberAvatar" src="{{item.userInfo.avatarUrl}}"></image>
					<view class="memberText">
						<text class="memberName">{{item.userInfo.nickName}}</text>
						<text class="memberState">{{item.userInfo.qualifyState}}</text>
						<view class="memberQualifyState">
							<view wx:if="{{item.userInfo.isQualified}}" class="qualifyText-container">
								<text class="qualifyText">{{item.userInfo.qualifyText}}</text>
								<image src="../../images/qualify.png" class="qualifyIcon"></image>
							</view>
						</view>
					</view>
				</view>
			</view>
		</scroll-view>
	</view>

	<!-- 按钮们 -->
	<view class="projectDetailBottomButtonsLine" style="display:{{disProjectDetailBottomLine}}">
		<view style="height:140rpx;width:60%;display:inline-block;">
			<view class="projectDetailBottomButton" bindtap="tapGood" data-proid="{{chooseProject._id}}" data-n="1">
				<image wx:if="{{!chooseProject.isGood}}" class="mediumIcon" src="../../images/项目详情底部栏-点赞按钮（未点）@2x.png"></image>
				<image wx:if="{{chooseProject.isGood}}" class="mediumIcon" src="../../images/项目详情底部栏-点赞按钮（选中）@2x.png"></image>
				<text class="projectDetailBottomNum">{{chooseProject.goodNum}}</text>
