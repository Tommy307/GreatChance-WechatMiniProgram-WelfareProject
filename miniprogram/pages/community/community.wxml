<!-- 社区页 -->
<view style="display:{{disCommunity}}">
	<view class="topBar">
		<view class="searchContainer" style="margin-bottom:50rpx;width:{{searchContainerW}};text-align:left;"
			bindtap="onTapSearch">
			<input class="searchInput" style="width:{{searchInputW}};" focus="{{focusInput}}" placeholder="{{searchHolder}}"
				bindinput="searchInput" value="{{inputValue}}" placeholder-style="color:#aaa;"></input>
		</view>
		<image class="quxiaoIcon" style="display:{{disQXIcon}};" src="../../images/quxiao.png" bindtap="onTapQXIcon">
		</image>
		<image class="searchIcon" style="display:{{disSIcon}}" src="../../images/sousuo.png" bindtap="onTapSearchIcon">
		</image>
		<text class="topBarText" style="display:{{disSIcon}}">谷仓计划</text>
		<text class="quxiaoText" style="display:{{disQXText}}" bindtap="onTapQXText">取消</text>
	</view>

	<!-- 搜索结果 -->
	<view style="margin-top:180rpx;">
		<view wx:for="{{searchResultProjects}}" class="project-container">
			<!-- 点击主体部分进入详情页 -->
			<view bindtap="onTapProjectListOne" data-proid="{{item._id}}" data-sign="search" data-n="2">
				<view class="projectName">
					<text>{{item.projectName}}</text>
				</view>
				<view class="projectStateLine">
					<span class="projectStateText">状态:{{item.projectState}}</span>
					<span class="projectStateText">起止时间:{{item.startDate}}~{{item.endDate}}</span>
				</view>

				<view class="projectViewCon">
					<view style="width:30%;height:210rpx;display:inline-block;">
						<image class="image-container" style="width:85%;height:82%;margin-left:20rpx"
							src='{{item.projectCoverImageID}}'></image>
					</view>
					<view style="width:70%;height:210rpx;display:inline-block;border-radius:10rpx;">
						<text class="projectInformation"
							style="width:88%;height:80%;margin-top:10rpx;">{{item.projectInformation}}</text>
					</view>
				</view>

			</view>
			<view class="projectBottomButtonsLine">
				<view class="projectBottomButton">
					<image wx:if="{{!item.isGood}}" class="smallIcon" src="../../images/项目详情底部栏-点赞按钮（未点）@2x.png" bindtap="tapGood"
						data-proid="{{item._id}}"></image>
					<image wx:if="{{item.isGood}}" class="smallIcon" src="../../images/项目详情底部栏-点赞按钮（选中）@2x.png"></image>
					<text class="projectBottomNum">{{item.goodNum}}</text>
				</view>
				<!-- 发表评论按钮 -->
				<view class="projectBottomButton">
					<image class="smallIcon" src="../../images/项目详情底部栏-评论按钮@2x.png" bindtap="tapComment"
						data-proid="{{item._id}}"></image>
					<text class="projectBottomNum">{{item.comments.length}}</text>
				</view>
				<view class="projectBottomButton">
					<image wx:if="{{!item.isCollected}}" class="smallIcon" src="../../images/项目详情底部栏-收藏按钮（未点）@2x.png"
						bindtap="tapCollect" data-proid="{{item._id}}"></image>
					<image wx:if="{{item.isCollected}}" class="smallIcon" src="../../images/项目详情底部栏-收藏按钮（选中）@2x.png"></image>
					<text class="projectBottomNum">{{item.collectNum}}</text>
				</view>
			</view>
		</view>
	</view>

	<!-- head -->
	<view class="head" style="display:{{disHead}}">
		<view class="divideText"
			style="left:-80rpx;font-weight:{{pageWeight[0]}};border-bottom:{{pageBorder[0]}};color:#999;"
			bindtap="onTapDiscoverPageText">发现</view>
		<view class="divideText" style="font-weight:{{pageWeight[1]}};border-bottom:{{pageBorder[1]}};color:#999;"
			bindtap="onTapFavorPageText">关注</view>
		<view class="divideText"
			style="left:80rpx;font-weight:{{pageWeight[2]}};border-bottom:{{pageBorder[2]}};color:#999;"
			bindtap="onTapTopicPageText">话题</view>
	</view>

	<!-- body -->
	<scroll-view class="scroll-body" scroll-y="true" style="display:{{disBody}}"
		bindtouchstart="communitySharePullDownToRefresh" bindscroll="communitySharePull">
		<view>
			<!-- Banner -->
			<swiper wx:if="{{bannerProjects.length!=0}}" style="display:{{displayBanner}}" class="projectBanner"
				indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}"
				circular="true">
				<view wx:for="{{bannerProjects}}">
					<swiper-item bindtap="onTapProjectListOne" data-proid="{{item.pro._id}}" data-n="4">
						<image src="{{item.pro.projectCoverImageID}}" class="mainProject" />
					</swiper-item>
				</view>
			</swiper>

			<!-- 四标签 -->
			<view style="display:{{displayLabels}};margin-top:20rpx">
				<view class="divContainer">
					<text class="divText">项目类型</text>
				</view>
				<view style="margin-top:5rpx;">
					<view>
						<image src="{{labelImg[0]}}" class="projectEntrance" style="background-color:{{labelColor[0]}};left:34rpx;"
							bindtap="tapProjectLabel" data-n="0"></image>
						<image src="{{labelImg[1]}}" class="projectEntrance" style="background-color:{{labelColor[1]}};left:36rpx;"
							bindtap="tapProjectLabel" data-n="1"></image>
					</view> <!-- 乡村教育、振兴 -->
					<view>
						<image src="{{labelImg[2]}}" class="projectEntrance" style="background-color:{{labelColor[2]}};left:34rpx;"
							bindtap="tapProjectLabel" data-n="2"></image>
						<image src="{{labelImg[3]}}" class="projectEntrance" style="background-color:{{labelColor[3]}};left:36rpx;"
							bindtap="tapProjectLabel" data-n="3"></image>
					</view> <!-- 志愿招募 基层调研 -->
				</view> <!-- 四标签 -->
			</view>

			<!-- 资讯流 -->
			<view class="sharesStream" style="margin-top:20rpx">
				<view class="divContainer" style="margin-bottom:5rpx">
					<text class="divText">{{zixunText}}</text>
				</view>

				<!-- 社区资讯流 -->
				<view wx:for="{{shares}}" class="share-container" style="display:{{disZixun1}}">
					<!-- 用户信息 头像 昵称 验证信息 -->
					<view class="userInfoContainer">
						<image class="userAvatar" src="{{item._user.avatarUrl}}" bindtap="enterUserDetail"
							data-userid="{{item._userID}}"></image>
						<text class="userName" style="top:-65rpx">{{item._user.nickName}}</text>
					</view>
					<!-- 资讯的内容 -->
					<view class="shareContent">
						<text class="shareTopic" bindtap="onClickTopic"
							data-topic="{{item.shareTopic}}">#{{item.shareTopic}}#</text>
						<text class="shareSubject">{{item.shareSubject}}</text>
						<text class="shareInformation">{{item.shareInformation}}</text>
						<view wx:if="{{item.imageID.length>0}}">
							<image class="image-container" src="{{item.imageID[0]}}" bindtap="viewImage"
								data-imageurl="{{item.imageID[0]}}"></image>
							<image class="image-container" src="{{item.imageID[1]}}" bindtap="viewImage"
								data-imageurl="{{item.imageID[1]}}"></image>
							<image class="image-container" src="{{item.imageID[2]}}" bindtap="viewImage"
								data-imageurl="{{item.imageID[2]}}"></image>
						</view>
					</view>
					<!-- 底部按钮区 -->
					<view class="shareBottomButtonsLine">
						<view class="projectBottomButton">
							<image wx:if="{{!item.isGood}}" class="smallIcon" src="../../images/项目详情底部栏-点赞按钮（未点）@2x.png"
								bindtap="tapGood" data-shareid="{{item._id}}"></image>
							<image wx:if="{{item.isGood}}" class="smallIcon" src="../../images/项目详情底部栏-点赞按钮（选中）@2x.png"></image>
							<text class="projectBottomNum">{{item.goodNum}}</text>
						</view>
						<!-- 发表评论按钮 -->
						<view class="projectBottomButton">
							<image class="smallIcon" src="../../images/项目详情底部栏-评论按钮@2x.png" bindtap="tapComment"
								data-shareid="{{item._id}}"></image>
							<text class="projectBottomNum">{{item.comments.length}}</text>
						</view>
					</view>
				</view>

				<!-- 没有关注时的图片 -->
				<view wx:if="{{focus_shares.length==0 && pageWeight[1]=='bold'}}">
					<image class="fittingImage"
						src="cloud://greatchancecloud-5lau2.6772-greatchancecloud-5lau2-1301741948/projectImages-42d70ff05e843f7a003004b0740a94a7-202047184052021.83719264319666">
					</image>
					<text class="fittingImageText">您还没有关注的用户</text>
					<text class="fittingImageText">点击用户头像即可关注用户啦</text>
				</view>

				<!-- 关注的人的资讯流 -->
				<view wx:for="{{focus_shares}}" class="share-container" style="display:{{disZixun2}}">
					<!-- 用户信息 头像 昵称 验证信息 -->
					<view class="userInfoContainer">
						<image class="userAvatar" src="{{item._user.avatarUrl}}" bindtap="enterUserDetail"
							data-userid="{{item._userID}}"></image>
						<text class="userName" style="top:-65rpx">{{item._user.nickName}}</text>
					</view>
					<!-- 资讯的内容 -->
					<view class="shareContent">
						<text class="shareTopic" bindtap="onClickTopic"
							data-topic="{{item.shareTopic}}">#{{item.shareTopic}}#</text>
						<text class="shareSubject">{{item.shareSubject}}</text>
						<text class="shareInformation">{{item.shareInformation}}</text>
						<view wx:if="{{item.imageID.length>0}}">
							<image class="image-container" src="{{item.imageID[0]}}" bindtap="viewImage"
								data-imageurl="{{item.imageID[0]}}"></image>
							<image class="image-container" src="{{item.imageID[1]}}" bindtap="viewImage"
								data-imageurl="{{item.imageID[1]}}"></image>
							<image class="image-container" src="{{item.imageID[2]}}" bindtap="viewImage"
								data-imageurl="{{item.imageID[2]}}"></image>
						</view>
					</view>
					<!-- 底部按钮区 -->
					<view class="shareBottomButtonsLine">
						<view class="projectBottomButton">
							<image class="smallIcon" src="../../images/项目详情底部栏-点赞按钮（未点）@2x.png"></image>
							<text class="projectBottomNum">{{item.goodNum}}</text>
						</view>
						<!-- 发表评论按钮 -->
						<view class="projectBottomButton">
							<image class="smallIcon" src="../../images/项目详情底部栏-评论按钮@2x.png" bindtap="tapComment"
								data-shareid="{{item._id}}"></image>
							<text class="projectBottomNum">{{item.comments.length}}</text>
						</view>
					</view>
				</view>
			</view>

			<!-- 话题 -->
			<view wx:for="{{topics}}" style="display:{{displayTopics}}">
				<view class="topic-container" bindtap="tapComment" data-topicid="{{item._id}}">
					<text class="topic">#{{item.topic}} #</text>
				</view>
			</view>
			<view class="endLineText">已经到底了</view>
		</view>
	</scroll-view>
</view>

<!-- 项目管理页 -->
<view class="manageProjectPage" style="display:{{disManageProjectPage}}">
	<view class="topBar">
		<image class="messageIcon" src="../../images/xiaoxitongzhi.png" bindtap="onTapMessageIcon"></image>
		<view class="unreadIcon" style="display:{{disUnread}}"></view>
		<text class="topBarText">谷仓计划</text>
	</view>

	<view class="head">
		<view class="divideText" bindtap="onTapMyProject"
			style="left:-80rpx;font-weight:{{mpageWeight[0]}};border-bottom:{{mpageBorder[0]}};#999;">发起</view>
		<view class="divideText" bindtap="onTapParticipateProject"
			style="font-weight:{{mpageWeight[1]}};border-bottom:{{mpageBorder[1]}};#999">参与</view>
		<view class="divideText" bindtap="onTapFavorProject"
			style="left:80rpx;font-weight:{{mpageWeight[2]}};border-bottom:{{mpageBorder[2]}};#999">收藏</view>
	</view>

	<scroll-view class="scroll-body" scroll-y="true">
		<view wx:if="{{manageProjectPageDisplayProjects.length==0 && mpageBorder[0]==''}}">
			<image class="fittingImage"
				src="cloud://greatchancecloud-5lau2.6772-greatchancecloud-5lau2-1301741948/projectImages-42d70ff05e843f7a003004b0740a94a7-202047183847075.80996860807936">
			</image>
			<text class="fittingImageText">您没有发起的项目</text>
			<text class="fittingImageText">点击右下方的 [+] 发起自己的项目吧</text>
		</view> <!-- 未发布 -->
		<view wx:if="{{manageProjectPageDisplayProjects.length==0 && mpageBorder[1]==''}}">
			<image class="fittingImage"
				src="cloud://greatchancecloud-5lau2.6772-greatchancecloud-5lau2-1301741948/projectImages-42d70ff05e843f7a003004b0740a94a7-20204718386099.11533228023772">
			</image>
			<text class="fittingImageText">您还没有参与的项目</text>
			<text class="fittingImageText">到 [社区] - [发现] 参与感兴趣的项目吧</text>
		</view> <!-- 未参与 -->
		<view wx:if="{{manageProjectPageDisplayProjects.length==0 && mpageBorder[2]==''}}">
			<image class="fittingImage"
				src="cloud://greatchancecloud-5lau2.6772-greatchancecloud-5lau2-1301741948/projectImages-42d70ff05e843f7a003004b0740a94a7-202047183847174.18220652343877">
			</image>
			<text class="fittingImageText">您还没有收藏的项目</text>
			<text class="fittingImageText">到 [社区] - [发现] 收藏感兴趣的项目吧</text>
		</view> <!-- 未收藏 -->

		<view wx:for="{{manageProjectPageDisplayProjects}}" class="project-container">
			<!-- 点击主体部分进入详情页 -->
			<view bindtap="onTapProjectListOne" data-proid="{{item._id}}" data-n="1">
				<view class="projectName">
					<text>{{item.projectName}}</text>
				</view>
				<view class="projectStateLine">
					<span class="projectStateText">状态:{{item.projectState}}</span>
					<span class="projectStateText">起止时间:{{item.startDate}}~{{item.endDate}}</span>
				</view>

				<view class="projectViewCon">
					<view style="width:30%;height:210rpx;display:inline-block;">
						<image class="image-container" style="width:85%;height:82%;margin-left:20rpx"
							src='{{item.projectCoverImageID}}'></image>
					</view>
					<view style="width:70%;height:210rpx;display:inline-block;border-radius:10rpx;">
						<text class="projectInformation"
							style="width:88%;height:80%;margin-top:10rpx;">{{item.projectInformation}}</text>
					</view>
				</view>
			</view>
			<view class="projectBottomButtonsLine">
				<!-- 点赞按钮 -->
				<view class="projectBottomButton">
					<image wx:if="{{!item.isGood}}" class="smallIcon" src="../../images/项目详情底部栏-点赞按钮（未点）@2x.png" bindtap="tapGood"
						data-proid="{{item._id}}" data-n="2"></image>
					<image wx:if="{{item.isGood}}" class="smallIcon" src="../../images/项目详情底部栏-点赞按钮（选中）@2x.png"></image>
					<text class="projectBottomNum">{{item.goodNum}}</text>
				</view>
				<!-- 发表评论按钮 -->
				<view class="projectBottomButton">
					<image class="smallIcon" src="../../images/项目详情底部栏-评论按钮@2x.png" bindtap="tapComment"
						data-proid="{{item._id}}"></image>
					<text class="projectBottomNum">{{item.comments.length}}</text>
				</view>
				<!-- 收藏按钮 -->
				<view class="projectBottomButton">
					<image wx:if="{{!item.isCollected}}" class="smallIcon" src="../../images/项目详情底部栏-收藏按钮（未点）@2x.png"
						bindtap="tapCollect" data-proid="{{item._id}}" data-n="2"></image>
					<image wx:if="{{item.isCollected}}" class="smallIcon" src="../../images/项目详情底部栏-收藏按钮（选中）@2x.png"></image>
					<text class="projectBottomNum">{{item.collectNum}}</text>
				</view>
			</view>
		</view>

		<view class="messages">
			<view class="messageContainer" style="display:{{disMessage}}" wx:for="{{unreadMessages}}" wx:key="none">
				<view class="message" style="background-color:grey">
					<text>{{item.detail}}-{{item.projectName}}</text>
					<view>
						<button class="tinyButton" style="left:60rpx;" bindtap="onAgreeJoinProject"
							data-projectid="{{item.projectId}}" data-projectname="item.projectName"
							data-applicantid="{{item.applicantId}}">同意</button>
						<button class="tinyButton" style="left:90rpx" bindtap="onRejectJoinProject">拒绝</button>
						<button class="tinyButton" style="left:120rpx">忽略</button>
					</view>
				</view>
			</view>
		</view>

		<view wx:if="{{manageProjectPageDisplayProjects.length!=0}}" class="endLineText">已经到底了</view>
	</scroll-view>

	<!-- 消息面板 -->
	<view class="message-container" style="display:{{disMessage}}">
		<view class="quxiaoLine">
			<image class="quxiaoIcon2" src="../../images/quxiao.png" bindtap="onTapCancelMess"></image>
		</view>
		<scroll-view scroll-y="true" style="height:785rpx;">
			<view wx:for="{{unreadMess}}">
				<view class="message" bindtap="onTapProjectMessage" data-proid="{{item.proid}}" data-n="1">
					<image class="commentUserAvatar" src="{{item.userInfo.avatarUrl}}" bindTap="enterUserDetail"
						data-userid="item.user_id"></image>
					<view class="commentUserName">{{item.userInfo.nickName}}</view>
					<view class="commentUserName" style="color:#777"> {{item.time}}</view>
					<view class="commentContent">{{item.content}}</view>
					<view wx:if="{{item.needAgreeButton}}" bindtap="onTapMessButton" class="agreeButton"
						style="margin-left:220rpx" data-n="1" data-user="item.user_id" data-userinfo="{{item.userInfo}}"
						data-index="{{index}}">同意</view>
					<view wx:if="{{item.needAgreeButton}}" bindtap="onTapMessButton" class="agreeButton" data-n="2"
						data-user="item.user_id" data-index="{{index}}">拒绝</view>
					<view wx:if="{{item.needAgreeButton}}" bindtap="onTapMessButton" class="agreeButton" data-user="item.user_id"
						data-index="{{index}}">忽略</view>
					<view wx:if="{{!item.needAgreeButton}}" bindtap="onTapMessButtonOk" class="agreeButton" data-index="{{index}}"
						style="margin-left:480rpx">好的</view>
				</view>
			</view>
			<view style="height:3rpx;width:100%;background-color:white;"></view> <!-- 分割线 -->
			<view wx:for="{{readedMess}}" style="margin-top:15rpx">
				<view class="rmessage" bindtap="onTapProjectMessage" data-proid="{{item.proid}}" data-n="1">
					<image class="commentUserAvatar" src="{{item.userInfo.avatarUrl}}" bindtap="enterUserDetail"
						data-userid="item.user_id"></image>
					<view class="commentUserName">{{item.userInfo.nickName}}</view>
					<view class="commentUserName" style="color:#ddd"> {{item.time}}</view>
					<view class="commentContent">{{item.content}}</view>
					<view wx:if="{{item.needAgreeButton}}" bindtap="onTaprMessButton" class="agreeButton"
						style="margin-left:350rpx" data-n="1" data-user="item.user_id" data-userinfo="{{item.userInfo}}"
						data-index="{{index}}">同意</view>
					<view wx:if="{{item.needAgreeButton}}" bindtap="onTapMessButton" class="agreeButton" data-n="2"
						data-user="item.user_id" data-index="{{index}}">拒绝</view>
				</view>
			</view>
		</scroll-view>
	</view>

</view>

<!-- 我的页面 -->
<view class="myPage" style="display:{{disMyPage}}">
	<view class="topBar">
		<text class="topBarText">谷仓计划</text>
	</view>

	<view style="hight:600rpx;margin-top:135rpx;text-align:center;">
		<!-- 状态栏 -->
		<image src="{{userInfo.avatarUrl}}" class="myPageAvatar"></image>
		<text class="myPageName">{{userInfo.nickName}}</text>
		<text class="myPageQualifyState" style="margin-right:30rpx;">{{qualifyState}}</text>
		<view wx:if="{{!isQualified}}" class="qualifyButton" bindtap="onTapToQualify">认证</view>
		<view wx:if="{{isQualified}}" class="qualifyText-container">
			<text class="qualifyText">{{qualifyText}}</text>
			<image src="../../images/qualify.png" class="qualifyIcon"></image>
		</view>
		<!-- 个性签名 -->
		<view>
			<view class="userWords" style="display:{{disUserWords}}">
				<text>{{userWords}}</text>
			</view>
			<view class="userWords" style="display:{{disUserWordsInput}};text-align:left;">
				<textarea style="width:97%;height:110rpx;line-height:35rpx;margin:5rpx 10rpx;font-size:28rpx;"
					bindinput="onUserWordsInput" row="2" placeholder="0~50" maxlength="50"></textarea>
			</view>
			<image class="myPageIcon" style="position:relative;margin-left:5rpx;top:-20rpx;"
				src="../../images/editUserWords.png" bindtap="onTapEditUserWords"></image>
		</view>
		<!-- 用户历史记录 -->
		<view class="userProjectHistory" bindtap="quitEditUserWords">
			<view class="userInfoText">
				<image src="../../images/finish.png" class="myPageIcon" style="margin-right:5rpx"></image>发布并完成项目:
				{{history[0]}}
			</view>
			<view class="userInfoText">
				<image src="../../images/finish.png" class="myPageIcon" style="margin-right:5rpx"></image>参与并完成项目:
				{{history[1]}}
			</view>
			<view class="userInfoText">
				<image src="../../images/quit.png" class="myPageIcon" style="margin-right:5rpx"></image>发布但放弃项目: {{history[2]}}
			</view>
			<view class="userInfoText">
				<image src="../../images/quit.png" class="myPageIcon" style="margin-right:5rpx"></image>参与但退出项目: {{history[3]}}
			</view>
			<view style="width:100%;height:4rpx;background-color:#ddd;"></view>
		</view>
	</view>

	<!-- 功能选择栏 -->
	<view style="margin-top:35rpx" bindtap="quitEditUserWords">
		<view class="myPage-container" bindtap="onTapMyPageDiv" data-n="1">
			<image src="../../images/xiaoxi.png" class="myPageIcon" id="mess" style="margin-left: 40rpx;"></image>消息通知
			<view class="unreadIconMyPage" style="display:{{disUnread}}">{{unreadMess.length}}</view>
